  <simpleType name="TypeVegType">
    <annotation>
      <documentation>Type veg (FormOfWay).</documentation>
      <appinfo>
        <taggedValue xmlns="http://www.interactive-instruments.de/ShapeChange/AppInfo" tag="SOSI_navn">TYPEVEG</taggedValue>
